#!/usr/bin/env python3

SAMPLES     = ['ATACrep3', 'ATACrep4']
READ_PAIRS  = ['R1', 'R2']
RAW_READS   = expand('data/{sample}_{read}.fastq.gz', sample=SAMPLES, read=READ_PAIRS)


def get_final_output():
    final_list = expand('results/qc/fastqc/{sample}_{read}_fastqc.html', sample=SAMPLES, read=READ_PAIRS)
    return final_list
